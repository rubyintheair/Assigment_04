<p id="notice"><%= notice %></p>

<h4>Food Items</h4>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Description</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_items.each do |food_item| %>
      <tr>
        <td><%= food_item.name %></td>
        <td><%= number_to_currency(food_item.price, unit: "VND") %></td>
        <td><%= food_item.description %></td>
        <td><%= food_item.section.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


